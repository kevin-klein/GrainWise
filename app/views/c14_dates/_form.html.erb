<%= simple_form_for(@c14_date) do |f| %>
  <%= f.error_notification %>
  <%= if f.object.errors[:base].present?
    f.error_notification message: f.object.errors[:base].to_sentence
  end %>

  <div class="form-inputs">
    <%= f.input :c14_type %>
    <%= f.input :lab_id %>
    <%= f.input :age_bp %>
    <%= f.input :interval %>
    <%= f.input :material %>
    <%= f.input :calbc_1sigma_max %>
    <%= f.input :calbc_1_sigma_min %>
    <%= f.input :calbc_2sigma_max %>
    <%= f.input :calbc_2sigma_min %>
    <%= f.input :date_note %>
    <%= f.input :cal_method %>
    <%= f.input :ref_14c %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
